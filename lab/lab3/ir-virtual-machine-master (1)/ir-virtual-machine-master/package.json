        "@typescript-eslint/parser": "^7.6.0",
        "@vitejs/plugin-react": "^3.1.0",
        "babel-jest": "^29.5.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "jest": "^29.5.0",
        "rollup": "^4.13.0",
        "sass": "^1.62.0",
        "typescript": "^4.9.3",
        "typescript-eslint": "^7.7.1",
